<div>
    <main>
        <section>
            <div class="card">
                <div class="card-body">
                    <h1 class="card-title">{{heading.main.title}}</h1>
                    <p class="card-text">{{heading.main.text}}</p>
                    <button type="button" class="btn btn-primary">{{heading.main.button_text.solid}}</button>
                    <button type="button" class="btn btn-info">{{heading.main.button_text.outline}}</button>
                </div>

                <div class="card-body">
                    <h2>{{heading.second.title}}</h2>
                    <p>{{heading.second.text}}</p>
                    <button type="button" class="btn btn-primary">{{heading.second.button_text.solid}}</button>
                    <button type="button" class="btn btn-info">{{heading.second.button_text.outline}}</button>
                </div>

                <div class="card-body">
                    <h2 class="card-title">{{heading.subblock.title}}</h2>
                    <p>{{heading.subblock.text}}</p>
                    <button type="button" class="btn btn-primary">{{heading.subblock.button_text.solid}}</button>
                    <button type="button" class="btn btn-info">{{heading.subblock.button_text.outline}}</button>
                </div>

                <div class="card-body">
                    <h2>{{nested_display.title}}</h2>
                    <div class="card">
                        <h3>{{nested_display.children_display.title}}</h3>
                        <p>{{nested_display.children_display.text}}</p>
                        <button type="button"
                            class="btn btn-primary">{{nested_display.children_display.button.text}}</button>
                    </div>
                    <br>
                    <p>{{nested_display.header_text}}</p>
                    <button type="button" class="btn btn-primary">{{nested_display.button.text}}</button>
                </div>

                <div class="card-body">
                    <h2>{{paragraph.title}}</h2>
                    <p>{{paragraph.text}}</p>
                    <button type="button" class="btn btn-light"><a href="{{paragraph.button.url}}"
                            target="_blank">{{paragraph.button.text}}</a></button>
                    <ul>
                        {{#each paragraph.list_name}}
                        <li><a href="{{this.url}}" target="_blank">{{this.text}}</a></li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </section>
    </main>
</div>